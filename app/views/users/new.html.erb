
<h1>Sign Up</h1>

<%= form_with url: "/users" do |form| %>
  <div>
    <%= form.label "username" %>
    <%= form.text_field "username" %>
  </div>
  <div>
    <%= form.label "email" %>
    <%= form.text_field "email" %>
  </div>
  <div>
    <%= form.label "password" %>
    <%= form.password_field "password" %>
  </div>
  <%= form.submit "Sign Up" %>
<% end %>
