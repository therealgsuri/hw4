
<h1><%= @place["name"] %></h1>

<h2>My Entries</h2>

<% @entries.each do |entry| %>
  <div>
    <h3><%= entry["title"] %></h3>
    <p>Date: <%= entry["occurred_on"] %></p>
    <p><%= entry["description"] %></p>
  </div>
<% end %>

<p>
  <a href="/entries/new?place_id=<%= @place["id"] %>">New Entry</a>
</p>

<p>
  <a href="/places">Back to Places</a>
</p>
